{
    "parser": "@typescript-eslint/parser",
    "extends": [
        "next/core-web-vitals",
        "prettier",
        "eslint-config-airbnb-base",
        "eslint-config-prettier"
    ],
    "rules": {
        "import/extensions": [
            "error",
            "ignorePackages",
            {
                "js": "never",
                "jsx": "never",
                "ts": "never",
                "tsx": "never"
            }
        ]
    }
}
